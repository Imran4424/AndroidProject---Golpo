<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imu.golpo" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/golpo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.imu.golpo.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Splash"
            android:label="Golpo" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".kukur"
            android:label="@string/title_activity_kukur" >
        </activity>
        <activity
            android:name=".Bang"
            android:label="@string/title_activity_bang" >
        </activity>
        <activity
            android:name=".Jele"
            android:label="@string/title_activity_jele" >
        </activity>
    </application>

</manifest>
